
<h3>Edit Tree</h3>

<form action="/trees/<%= @tree['id'] %>" method="post">

  <label>Tree Name</label>
  <input type="text" name="name"
  value="<%= @tree['name'] %>">

  <label>Image url</label>
  <textarea name="image_url" rows="8" cols="40">
    <%= @tree['image_url'] %>
  </textarea>

  <button>Edit</button>

</form>
