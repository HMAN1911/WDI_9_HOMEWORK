<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Bar-Code Movies</title>
    <link rel="stylesheet" href="../public/stylesheets/style.css" media="screen" title="no title">
    <link href="https://fonts.googleapis.com/css?family=Cherry+Cream+Soda" rel="stylesheet">
  </head>
  <body>
    <% include ./partials/header.ejs %>
    <div class="container">
      <% movies.forEach(function(movie) { %>
        <div class="col-2 col-m-4 list-item">

          <a href="#"><img class=" small-poster" src="<%= movie.Poster %>" alt="" /></a>

          <p class="search-description"><%= movie.Title %></p>

        </div>
      <% }) %>

    </div>
    <% include ./partials/footer.ejs %>
  </body>
</html>
