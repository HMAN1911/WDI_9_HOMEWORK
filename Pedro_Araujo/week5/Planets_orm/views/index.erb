<div class="subtitle">
  <h2>List of Planets</h2>
</div>
<div class="">


<% @planets.each do |planet| %>


  <div class="planet-div col-6 col-m-6">
    <h3><%= planet["name"] %></h3>
    <a href="/planet/<%= planet['id'] %>"><img  class="planet" src="<%= planet['image_url'] %>" alt="" /></a>
    <p>
      Name: <%= planet["name"] %> <br>
      Moons: <%= planet["moons"] %>
    </p>
  </div>


<% end %>
</div>
